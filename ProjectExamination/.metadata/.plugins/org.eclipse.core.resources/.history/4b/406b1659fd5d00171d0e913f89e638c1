import java.util.ArrayList;

import com.ectumotech.upd.controll.*;

public class TestUPD {
	public static void main(String[] args) {
		/**
		 * 初始化数据
		 */
		Initdata initdata = new Initdata();
		
		/**
		 * 测试显示菜单
		 */
		UIshow UPD = new UIshow();
